# PhysicsGPT Environment Variables
# Copy this file to .env and fill in your actual API keys

# Required - OpenAI API Key
OPENAI_API_KEY=sk-your-openai-api-key-here

# Optional - For web search capabilities
TAVILY_API_KEY=tvly-your-tavily-api-key-here

# Optional - For LangChain tracing and monitoring
LANGCHAIN_API_KEY=lsv2_your-langchain-api-key-here
LANGCHAIN_TRACING_V2=true
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com
LANGCHAIN_PROJECT=physics-gpt

# Model Configuration
PHYSICS_AGENT_MODEL=gpt-4o-mini
PHYSICS_AGENT_TEMPERATURE=0.1